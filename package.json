{
  "name": "translation-demo",
  "version": "0.0.1",
  "private": true,
  "description": "Google Cloud AI Speech, Text-To-Speech, Translation, Material-UI, React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "bundle": "webpack --config webpack.config.js",
    "prestart": "npm run bundle",
    "build": "webpack --mode production",
    "devstart": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "npm run build && node src/server/index.js",
    "lint": "eslint '**/*.js'",
    "fix": "eslint --fix '**/*.js'"
  },
  "author": "Kristin Galvin",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@google-cloud/speech": "^2.2.0",
    "@google-cloud/text-to-speech": "^0.5.0",
    "@google-cloud/translate": "^2.1.3",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.3.1",
    "circular-array": "^0.1.1",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "express": "^4.16.3",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "path": "^0.12.7",
    "react": "^16.5.2",
    "react-countdown-now": "^2.1.0",
    "react-dom": "^16.5.2",
    "react-scripts": "2.1.5",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "socket.io-stream": "^0.9.1",
    "style-loader": "^0.23.0",
    "typeface-roboto": "0.0.54",
    "url-loader": "^1.0.1",
    "wav": "^1.0.2",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.2.0",
    "eslint": "^5.15.3",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.15.2"
  }
}
